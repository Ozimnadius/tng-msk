                </svg>
            a(href="/catalog/").menu-catalog__section-link Меню
        .menu-catalog__list
            -
                var subitems = [
                    'Топпинги',
                    'Холодные закуски',
                    'Закуски',
                    'Соусы',
                    'Салаты',
                    'Супы',
                    'Паста',
                ];
