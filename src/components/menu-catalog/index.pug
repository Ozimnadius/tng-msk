.menu-catalog
    .menu-catalog__wrap
        .menu-catalog__top
            .menu-catalog__logo
                img(src="/images/content/logo.svg" alt="lorem")
            button(type='button' data-event='click.closeMenuCatalog').menu-catalog__close
                <svg width="45" height="45" viewBox="0 0 45 45" fill="none" xmlns="http://www.w3.org/2000/svg">
                <rect x="7" y="6" width="45" height="2" transform="rotate(45 7 6)" fill="/catalog/DFDFDF"/>
                <rect x="6" y="38" width="45" height="2" transform="rotate(-45 6 38)" fill="/catalog/DFDFDF"/>
                </svg>
        .menu-catalog__section
            button(type='button' data-event='click.closeMenuCatalog').menu-catalog__back
                <svg width="30" height="30" viewBox="0 0 30 30" fill="none" xmlns="http://www.w3.org/2000/svg">
